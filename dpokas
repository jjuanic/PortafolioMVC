[32m+[m[32musing Portafolio.Servicios;[m
[32m+[m
[32m+[m[32mvar builder = WebApplication.CreateBuilder(args);[m
[32m+[m
[32m+[m[32m// Add services to the container.[m
[32m+[m[32mbuilder.Services.AddControllersWithViews();[m
[32m+[m[32mbuilder.Services.AddTransient<IRepositorioProyectos, RepositorioProyectos>();[m
[32m+[m
[32m+[m
[32m+[m[32mvar app = builder.Build();[m
[32m+[m
[32m+[m[32m// Configure the HTTP request pipeline.[m
[32m+[m[32mif (!app.Environment.IsDevelopment())[m
[32m+[m[32m{[m
[32m+[m[32m    app.UseExceptionHandler("/Home/Error");[m
[32m+[m[32m    // The default HSTS value is 30 days. You may want to change this for production scenarios, see https://aka.ms/aspnetcore-hsts.[m
[32m+[m[32m    app.UseHsts();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mapp.UseHttpsRedirection();[m
[32m+[m[32mapp.UseStaticFiles();[m
[32m+[m
[32m+[m[32mapp.UseRouting();[m
[32m+[m
[32m+[m[32mapp.UseAuthorization();[m
[32m+[m
[32m+[m[32mapp.MapControllerRoute([m
[32m+[m[32m    name: "default",[m
[32m+[m[32m    pattern: "{controller=Home}/{action=Index}/{id?}");[m
[32m+[m
[32m+[m[32mapp.Run();[m
