[32m+[m[32musing System.Diagnostics;[m
[32m+[m[32musing Microsoft.AspNetCore.Mvc;[m
[32m+[m[32musing Portafolio.Models;[m
[32m+[m[32musing Portafolio.Servicios;[m
[32m+[m
[32m+[m[32mnamespace Portafolio.Controllers;[m
[32m+[m
[32m+[m[32mpublic class HomeController : Controller[m
[32m+[m[32m{[m
[32m+[m[32m    private readonly ILogger<HomeController> _logger;[m
[32m+[m[32m    private readonly IRepositorioProyectos repositorioProyectos;[m
[32m+[m
[32m+[m[32m    public HomeController(ILogger<HomeController> logger, IRepositorioProyectos repositorioProyectos)[m
[32m+[m[32m    {[m
[32m+[m[32m        _logger = logger;[m
[32m+[m[32m        this.repositorioProyectos = repositorioProyectos;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IActionResult Index()[m
[32m+[m[32m    {[m
[32m+[m[32m        var proyectos = repositorioProyectos.ObtenerProyectos().Take(3).ToList();[m
[32m+[m[32m        var modelo = new HomeIndexViewModel(){Proyectos = proyectos};[m
[32m+[m[32m        return View(modelo);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IActionResult Privacy()[m
[32m+[m[32m    {[m
[32m+[m[32m        return View();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IActionResult Proyectos()[m
[32m+[m[32m    {[m
[32m+[m[32m        var proyectos = repositorioProyectos.ObtenerProyectos();[m
[32m+[m[32m        return View(proyectos);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IActionResult Contacto()[m
[32m+[m[32m    {[m
[32m+[m[32m        return View();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    [HttpPost] // Atributo, si no estÃ¡, se sobreentiende que es un HttpGet[m
[32m+[m
[32m+[m[32m    public IActionResult Contacto(ContactoViewModel contactoViewModel)[m
[32m+[m[32m    {[m
[32m+[m[32m        return RedirectToAction("Gracias");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IActionResult Gracias()[m
[32m+[m[32m    {[m
[32m+[m[32m        return View();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    [ResponseCache(Duration = 0, Location = ResponseCacheLocation.None, NoStore = true)][m
[32m+[m[32m    public IActionResult Error()[m
[32m+[m[32m    {[m
[32m+[m[32m        return View(new ErrorViewModel { RequestId = Activity.Current?.Id ?? HttpContext.TraceIdentifier });[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
